{
  "name": "example-gulp-protractor",
  "version": "0.0.1",
  "description": "With TypeScript",
  "scripts": {
    "compile": "npm run tsc",
    "gulp": "node node_modules/gulp/bin/gulp.js",
    "test": "npm run compile && npm run gulp protractor",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "typings": "node ./node_modules/typings/dist/bin.js",
    "watch": "npm run tsc -- --watch"
  },
  "devDependencies": {
    "gulp": "latest",
    "gulp-angular-protractor": "latest",
    "jasmine-reporters": "2.2.0",
    "typescript": "1.8.10",
    "typings": "1.3.0"
  }
}
